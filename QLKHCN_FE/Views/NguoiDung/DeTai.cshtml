    function groupUser() {
        var table = document.getElementById("tbtacgia2");
        var groupUser = "";

        for (let i = 1; i < table.rows.length; i++) {
            const row = table.rows[i];

            for (let j = 0; j < row.cells.length; j++) {

                const cell = row.cells[j].textContent;
                if (cell == "") {
                    groupUser += cell + "Trống;";
