        } else {
            var scopusold = "https://apikhcn.uef.edu.vn/api/DanhMucScimago";
        }
        if (inputSearch == "") { }
        else {
            fetch(scopusold + "/Get-issn?issn=" + inputSearch, {
                headers: {
                    'Authorization': 'Bearer ' + _token,
                    'Content-Type': 'application/json'
                }
            })
