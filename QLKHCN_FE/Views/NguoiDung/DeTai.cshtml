                                        }
                                    })
                                        .then(function (res4) {
                                            if (res4.status == 404) {
                                                toastr.warning("Không tim thấy sản phẩm trong ISI, Scopus, HĐCDGSNN!");
                                                document.querySelector('#ketqua').innerHTML = ``;
                                                document.querySelector('#btn_Kissn').checked = true;
                                                click_radioIssn();
                                            } else {
                                                return res4.json();

                                            }
                                        })
                                        .then(callback)
                                }
                                else {
                                    return res3.json();
                                }

                            })
                            .then(callback)
                    }
                    else {
                        return res2.json();
                    }
                })
                .then(callback)

        }
